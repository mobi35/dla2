<script>



    document.getElementById("username").innerHTML = sessionStorage.getItem("username") + " - " + sessionStorage.getItem("role");

    if (sessionStorage.getItem("role") == "student") {
        $('#teacher-panel').css("display", "none");
        $('#student-panel').css("display", "block");

        $('#registrar-panel').css("display", "none");
        $('#admin-panel').css("display", "none");

    } else if (sessionStorage.getItem("role") == "Teacher") {
        $('#teacher-panel').css("display", "block");
        $('#student-panel').css("display", "none");

             $('#registrar-panel').css("display", "none");
        $('#admin-panel').css("display", "none");

    }

    else if (sessionStorage.getItem("role") == "Admin") {
        $('#teacher-panel').css("display", "none");
        $('#student-panel').css("display", "none");
              $('#registrar-panel').css("display", "none");
        $('#admin-panel').css("display", "block");

    }

    else if (sessionStorage.getItem("role") == "Registrar") {
        $('#teacher-panel').css("display", "none");
        $('#student-panel').css("display", "none");
            $('#registrar-panel').css("display", "block");
        $('#admin-panel').css("display", "none");

    }

    function LogoutNow() {

        sessionStorage.clear();
        window.open("../../../../", "_self");
    }


</script>

